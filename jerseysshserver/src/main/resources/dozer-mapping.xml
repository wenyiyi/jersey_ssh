<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
  <mapping>
    <class-a>com.wyd.domain.Employee</class-a>
    <class-b>com.wyd.domain.Record</class-b>
    <field>
      <a>name</a>
      <b>room</b>
    </field>
  </mapping>
  
   <mapping>
 	  <class-a>com.wyd.domain.Record</class-a>
  	  <class-b>com.wyd.domain.DateDozer</class-b>
  <field>
	  <a date-format="yyyy/MM/dd">start</a>
	  <b>date</b>
  </field>
</mapping>
</mappings>
